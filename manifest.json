{
    "manifest_version": 3,
    "name": "AI GMAIL EXTENSION",
    "version": "1.0",
    "description": "A basic Chrome extension for Gmail.",
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "https://mail.google.com/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }